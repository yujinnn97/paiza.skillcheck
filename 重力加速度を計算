import math
def calculateGravity(planet):
    if planet=="Earth":
        return 9.8
    elif planet=="Jupiter":
        return 24.79
    elif planet=="Mercury":
        return 3.7
    else:
        return 0
def fallingDistance(planet,time):
    # 関数を完成させてください
    h=math.floor((calculateGravity(planet)*time**2)/2)
    return math.floor(h*0.000621371)



